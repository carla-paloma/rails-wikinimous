<div class="container">
  <%= form_for(@article) do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :content %>
  <%= f.text_field :content %>
  <%= f.submit %>
<% end %>
</div>
<button type="button" class="btn btn-secondary">
        <%= link_to 'Back', articles_path  %>
</button>
